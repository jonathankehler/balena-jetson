+#include "tegra194-agx-cti-AGX101.dts"
+
+/{
+    nvidia,dtsfilename = "tegra194-agx-cti-AGX101-usb-dev.dts";
+
+    extcon_cti: extcon_cti {
+        reg = <0>;
+        #extcon-cells = <1>;
+        compatible = "linux,extcon-usb-gpio";
+        id-gpio = <&tegra_main_gpio TEGRA194_MAIN_GPIO(Q, 0) GPIO_ACTIVE_HIGH>;
+
+    };
+
+    xudc@3550000 {
+        phys = <&{/xusb_padctl@3520000/pads/usb2/lanes/usb2-0}>,
+            <&{/xusb_padctl@3520000/pads/usb3/lanes/usb3-2}>;
+        phy-names = "usb2", "usb3";
+        #extcon-cells = <1>;
+        nvidia,xusb-padctl = <&xusb_padctl>;
+        extcon-cable-names = "vbus";
+        extcon-cables = <&extcon_cti 0>;
+        status = "okay";
+    };
+
+    xusb_padctl@3520000 {
+        ports{
+            usb2-0 {
+                    mode = "otg";
+                };
+        };
+    };
+    xhci@3610000 {
+        extcon-cable-names = "id";
+        extcon-cables = <&extcon_cti 1>;
+        #extcon-cells = <1>;
+    };
+
+
+};
